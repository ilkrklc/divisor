{
  "name": "divisor",
  "version": "1.0.2",
  "description": "Calculates divisors/factors of natural numbers and gcd/lcm using euclid's algorithm.",
  "keywords": [
    "divisor",
    "common divisors",
    "greatest common divisor",
    "gcd",
    "greatest common factor",
    "gcf",
    "greatest common measure",
    "gcm",
    "least common multiple",
    "lowest common multiple",
    "smallest common multiple",
    "lcm",
    "euclidean algorithm",
    "euclid's algorithm"
  ],
  "homepage": "https://github.com/ilkrklc/divisor#readme",
  "bugs": {
    "url": "https://github.com/owner/project/issues",
    "email": "ilkrklc@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "İlker Kılıç",
    "email": "ilkrklc@gmail.com"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ilkrklc/divisor.git"
  },
  "scripts": {
    "test": "jest --config jest.config.js",
    "build": "rimraf dist && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "format": "prettier --write src",
    "lint": "eslint src/",
    "gen-docs": "typedoc --plugin typedoc-plugin-markdown src",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint && es-check es6 './dist/**/*.js'",
    "preversion": "npm run lint && npm run gen-docs",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "es-check": "^7.1.1",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "tscpaths": "0.0.9",
    "typedoc": "^0.25.1",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^5.2.2"
  },
  "types": "dist/index.d.ts"
}
